#ifndef XPCC_STM32_UART_TYPE_IDS_HPP
#define XPCC_STM32_UART_TYPE_IDS_HPP

namespace xpcc
{

namespace stm32
{

namespace TypeId
{
%% for id in [1,2,3,4,5,6]
	%% set uart = "Usart"
	%% if id in [4, 5]
		%% set uart = "Uart"
	%% endif
	typedef struct{} {{ uart }}Hal{{ id }}Rx;
	typedef struct{} {{ uart }}Hal{{ id }}Tx;
	typedef struct{} {{ uart }}Hal{{ id }}Ck;
	typedef struct{} {{ uart }}Hal{{ id }}Rts;
	typedef struct{} {{ uart }}Hal{{ id }}Cts;
	typedef struct{} Uart{{ id }}Rx;
	typedef struct{} Uart{{ id }}Tx;
	typedef struct{} Uart{{ id }}Ck;
	typedef struct{} Uart{{ id }}Rts;
	typedef struct{} Uart{{ id }}Cts;
%% endfor
}

}

}

#endif // XPCC_STM32_UART_TYPE_IDS_HPP
